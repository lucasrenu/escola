<VBox alignment="CENTER" spacing="20.0" minHeight="360" minWidth="360"  xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controllers.MenuController">
   <children>
      <Label text="MENU" />
      <Label fx:id="lblMSG" text="Menbros Ocultos" />
      <Button fx:id="btnCadastrar" onAction="#switchToSecondary" text="Ver menbros" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
